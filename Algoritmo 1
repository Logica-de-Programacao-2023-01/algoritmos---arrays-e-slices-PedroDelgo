package main

import "fmt"

// Crie um Array de inteiros com 3 elementos e imprima a soma dos valores armazenados no Array.
func main() {
	var soma int
	array := [3]int{1, 2, 3}
	for i := 0; i < len(array); i++ {
		soma += array[i]
	}
	fmt.Println("A soma dos valores armazenados na array Ã© igual a ", soma)
}
